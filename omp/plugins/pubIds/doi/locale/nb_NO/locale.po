msgstr "DOI kan ikke tildeles fordi det tilpassede suffikset mangler."

msgid "plugins.pubIds.doi.editor.assignDoi"
msgstr "Tilordne DOI {$pubId} til denne {$pubObjectType}"

msgid "plugins.pubIds.doi.editor.clearObjectsDoi.confirm"
msgstr "Er du sikker på at du vil slette eksisterende DOI?"

msgid "plugins.pubIds.doi.editor.clearObjectsDoi"
msgstr "Fjern"

msgid "plugins.pubIds.doi.editor.doiSuffixCustomIdentifierNotUnique"
msgstr ""
"Det spesifiserte DOI-suffikset er allerede i bruk sammen med et annet "
"publisert element. Skriv inn et unikt DOI-suffiks for hvert element."

msgid "plugins.pubIds.doi.editor.assigned"
msgstr "DOI er tildelt denne {$pubObjectType}."

msgid "plugins.pubIds.doi.editor.canBeAssigned"
msgstr ""
"Det du ser er en forhåndsvisning av DOI. Merk av i boksen og lagre skjemaet "
"for å bruke DOI."

msgid "plugins.pubIds.doi.editor.patternNotResolved"
msgstr "DOI kan ikke tildeles fordi den inneholder et uløselig mønster."

msgid "plugins.pubIds.doi.editor.missingParts"
msgstr ""
"Du kan ikke generere en DOI fordi en eller flere deler av DOI-mønsteret "
"mangler data."

msgid "plugins.pubIds.doi.editor.customSuffixMissing"
msgstr "DOI kan ikke tildeles fordi det brukerdefinerte suffikset mangler."

msgid "plugins.pubIds.doi.editor.doiObjectTypeSubmissionFile"
msgstr "fil"

msgid "plugins.pubIds.doi.editor.doiObjectTypeRepresentation"
msgstr "publiseringsformat"

msgid "plugins.pubIds.doi.editor.doiObjectTypeChapter"
msgstr "kapittel"

msgid "plugins.pubIds.doi.editor.doiObjectTypeSubmission"
msgstr "monografi"

msgid "plugins.pubIds.doi.editor.doi.assignDoi"
msgstr "Tildel"

msgid "plugins.pubIds.doi.editor.doi.description"
msgstr "DOI-en må begynne med {$prefix}."

