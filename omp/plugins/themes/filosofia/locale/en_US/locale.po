msgid "plugins.themes.filosofia.name"
msgstr "Filosofia Theme"

msgid "plugins.themes.filosofia.description"
msgstr "This theme implements the Filosofia theme."

msgid "plugins.themes.filosofia.option.typography.label"
msgstr "Typography"

msgid "plugins.themes.filosofia.option.typography.description"
msgstr "Choose a font combination that suits this press."

msgid "plugins.themes.filosofia.option.typography.notoSans"
msgstr ""
"Noto Sans: A digital-native font designed by Google for extensive language "
"support."

msgid "plugins.themes.filosofia.option.typography.notoSerif"
msgstr "Noto Serif: A serif variant of Google's digital-native font."

msgid "plugins.themes.filosofia.option.typography.notoSerif_notoSans"
msgstr ""
"Noto Serif/Noto Sans: A complementary pairing with serif headings and sans-"
"serif body text."

msgid "plugins.themes.filosofia.option.typography.notoSans_notoSerif"
msgstr ""
"Noto Sans/Noto Serif: A complementary pairing with sans-serif headings and "
"serif body text."

msgid "plugins.themes.filosofia.option.typography.lato"
msgstr "Lato: A popular modern sans-serif font."

msgid "plugins.themes.filosofia.option.typography.lora"
msgstr "Lora: A wide-set serif font good for reading online."

msgid "plugins.themes.filosofia.option.typography.lora_openSans"
msgstr ""
"Lora/Open Sans: A complimentary pairing with serif headings and sans-serif "
"body text."

msgid "plugins.themes.filosofia.option.typography.Verdana"
msgstr ""
"Verdana: Familiar font due to Microsoft Operating Systems"
"regular use."

msgid "plugins.themes.filosofia.option.colour.label"
msgstr "Colour"

msgid "plugins.themes.filosofia.option.colour.description"
msgstr "Choose a colour for the header."

msgid "plugins.themes.filosofia.option.useHomepageImageAsHeader.label"
msgstr "Header Background Image"

msgid "plugins.themes.filosofia.option.useHomepageImageAsHeader.description"
msgstr ""
"When a homepage image has been uploaded, display it in the background of the "
"header instead of it's usual position on the homepage."

msgid "plugins.themes.filosofia.option.useHomepageImageAsHeader.option"
msgstr "Show the homepage image as the header background."
